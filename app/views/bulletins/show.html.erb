<h2>Preview Bulletins</h2>

<table class="table table-bordered">
  <tr>
    <th>Title</th>
    <td><%=@bulletin.title %></td>
  </tr>
  <tr>
    <th>Description</th>
    <td><%= @bulletin.description %></td>
  </tr>
  <tr>
    <th>Created at</th>
    <td><%= @bulletin.created_at %></td>
  </tr>
</table>

<%= link_to 'Edit', edit_bulletin_path(@bulletin), class: 'btn btn-primary' %> 
<%= link_to 'Back', bulletins_path, class: 'btn btn-primary' %>
